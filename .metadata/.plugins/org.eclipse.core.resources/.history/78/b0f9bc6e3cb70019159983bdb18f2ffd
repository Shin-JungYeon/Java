package programmers;

public class Exam2 {

	public static void main(String[] args) {
		String arrangement = "()(((()())(())()))(())";
		int answer = 0;
		String[] arr = arrangement.split(arrangement, '(');
		for(String a : arr) {
			System.out.println(a);
		}
		System.out.println(arr.length);
	}

}
